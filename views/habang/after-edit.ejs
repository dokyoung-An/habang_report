    // ✅ 공종 선택 시 세부 공종 & 하자 내용 자동 로드
    function loadDefectOptions() {
        $(".sector").each(function () {
            const formId = $(this).data("form");
            const selectedSector = $(this).val();
            const specificSelect = $(`.specific[data-form="${formId}"]`);
            const contentSelect = $(`.content[data-form="${formId}"]`);

            specificSelect.empty().append('<option selected disabled>세부 공종</option>').prop("disabled", false);
            contentSelect.empty().append('<option selected disabled>하자 내용</option>').prop("disabled", true);

            if (defectData[selectedSector]) {
                Object.keys(defectData[selectedSector]).forEach(sub => {
                    specificSelect.append(`<option value="${sub}">${sub}</option>`);
                });

                // 기존 값이 있으면 선택
                const existingSpecific = specificSelect.data("existing-value");
                if (existingSpecific) {
                    specificSelect.val(existingSpecific).change();
                }
            }
        });
    }

    $(document).ready(function () {
        // ✅ 기존 데이터 로드 시 하자 공종 > 세부 공종 > 하자 내용 불러오기
        $(".specific").each(function () {
            const formId = $(this).data("form");
            const existingSpecific = $(this).val();
            $(this).data("existing-value", existingSpecific);
        });

        $(".content").each(function () {
            const formId = $(this).data("form");
            const existingContent = $(this).val();
            $(this).data("existing-value", existingContent);
        });

        loadDefectOptions();

        // ✅ 공종 선택 시 세부 공종 로드
        $(document).on("change", ".sector", function () {
            const formId = $(this).data("form");
            const selectedSector = $(this).val();
            const specificSelect = $(`.specific[data-form="${formId}"]`);
            const contentSelect = $(`.content[data-form="${formId}"]`);

            specificSelect.empty().append('<option selected disabled>세부 공종</option>').prop("disabled", false);
            contentSelect.empty().append('<option selected disabled>하자 내용</option>').prop("disabled", true);

            if (defectData[selectedSector]) {
                Object.keys(defectData[selectedSector]).forEach(sub => {
                    specificSelect.append(`<option value="${sub}">${sub}</option>`);
                });
            }
        });

        // ✅ 세부 공종 선택 시 하자 내용 로드
        $(document).on("change", ".specific", function () {
            const formId = $(this).data("form");
            const selectedSector = $(`.sector[data-form="${formId}"]`).val();
            const selectedSpecific = $(this).val();
            const contentSelect = $(`.content[data-form="${formId}"]`);

            contentSelect.empty().append('<option selected disabled>하자 내용</option>').prop("disabled", false);

            if (defectData[selectedSector] && defectData[selectedSector][selectedSpecific]) {
                defectData[selectedSector][selectedSpecific].forEach(defect => {
                    contentSelect.append(`<option value="${defect}">${defect}</option>`);
                });
            }
        });
    });


    
