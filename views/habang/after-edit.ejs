        let previewContainer = $(this).next(".preview-container");
        previewContainer.empty();

        let files = Array.from(event.target.files);
        if (files.length > 2) {
            alert("최대 2장까지 업로드할 수 있습니다.");
            event.target.value = ""; // 파일 선택 취소
            return;
        }

        files.forEach(file => {
            let reader = new FileReader();
            reader.onload = function (e) {
                previewContainer.append(`<img src="${e.target.result}" class="preview-image me-2">`);
            };
            reader.readAsDataURL(file);
        });
    });



